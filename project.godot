config_version=5

[application]

config/name="Struggle For Salvation"
config/tags=PackedStringArray("development")
run/main_scene="res://scenes/ui/menu_ui/main_menu.tscn"
config/features=PackedStringArray("4.1")
config/icon="res://icon.svg"

[autoload]

GlobalParameters="*res://scripts/global/global_parameters.gd"
BattleParameters="*res://scripts/global/battle_parameters.gd"
CardPlacementManager="*res://scripts/battle/base/phase_managers/card_placement_phase/card_placement_manager.gd"
CombatManager="*res://scripts/battle/base/phase_managers/combat_phase/combat_manager.gd"
PlayerState="*res://scripts/battle/base/player_state.gd"
HandlerForCardsPlacementByPlayer="*res://scripts/battle/base/phase_managers/card_placement_phase/handler_for_cards_placement_by_player.gd"
Settings="*res://scripts/global/settings/settings.gd"
Saves="*res://scripts/global/saves.gd"
SceneTransition="*res://scripts/global/scene_transition/scene_transition.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/resizable=false
window/stretch/mode="canvas_items"
window/size/fullscreen=true

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/SmoothScroll/plugin.cfg")

[filesystem]

import/blender/enabled=false

