window/size/fullscreen=true

[editor]

movie_writer/movie_file="C:/Godot/Projects/Struggle For Salvation/tests/test_movie.avi"

[editor_plugins]

enabled=PackedStringArray("res://addons/smooth_scroll/plugin.cfg")

[file_customization]

folder_colors={
"res://resources/": "pink",
"res://scenes/": "purple",
"res://scripts/": "blue",
"res://sprites/": "teal"
}

[filesystem]

import/blender/enabled=false

