[sub_resource type="Resource" id="Resource_nirkh"]
script = ExtResource("2_t55ri")
speaker_name = "Нейтрофил 763"
speaker_role = "Нейтрофил"
speakers = [SubResource("Resource_0y6gp")]
line = "Хорошо, я сделаю всё что смогу."

[sub_resource type="Resource" id="Resource_pm8h6"]
script = ExtResource("2_t55ri")
speaker_name = "Нейтрофил 480"
speaker_role = "Нейтрофил"
speakers = []
line = "И запомни, новобранец - твоя жизнь не важна, важна лишь целостность Организма."

