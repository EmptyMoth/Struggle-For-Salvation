expression = ExtResource("6_cowxw")

[sub_resource type="Resource" id="Resource_gc83g"]
script = ExtResource("2_t55ri")
speaker_name = "Нейтрофил 763"
speaker_role = "Нейтрофил"
speakers = [SubResource("Resource_6g8tn")]
line = "Что происходит? Почему меня привело сюда?"

[sub_resource type="Resource" id="Resource_5ovda"]
script = ExtResource("2_t55ri")
speaker_name = "Нейтрофил 480"
speaker_role = "Нейтрофил"
speakers = []
line = "Что? Ещё одного новобранца отправили на поле боя без стажировки. Да, дела у нас хуже некуда…"

[sub_resource type="Resource" id="Resource_aqh1i"]
script = ExtResource("2_t55ri")
speaker_name = "Нейтрофил 480"
speaker_role = "Нейтрофил"
speakers = []
line = "Слушай меня новобранец: на нас напал патоген – вирус гриппа. Мы находимся в лёгких - там, где он размножается."

[sub_resource type="Resource" id="Resource_pddco"]
script = ExtResource("2_t55ri")
speaker_name = "Нейтрофил 480"
speaker_role = "Нейтрофил"
speakers = []
line = "Вирус уже захватил несколько клеток эпителия дыхательных путей и продолжает распространяться. Мы должны его остановить. Задача предельно проста, выполняй."

[sub_resource type="Resource" id="Resource_78mca"]
script = ExtResource("3_ltbg8")
index = null
image = ExtResource("4_g7cdx")
expression = ExtResource("3_vjrky")

[sub_resource type="Resource" id="Resource_2lm23"]
script = ExtResource("2_t55ri")
speaker_name = "Нейтрофил 763"
speaker_role = "Нейтрофил"
speakers = [SubResource("Resource_78mca")]
line = "Я ещё ни разу не был в реальном бою. Как мне быть?"

[sub_resource type="Resource" id="Resource_g7spd"]
script = ExtResource("2_t55ri")
speaker_name = "Нейтрофил 480"
speaker_role = "Нейтрофил"
speakers = []
line = "Я это и без тебя понял, лишь по одному твоему поведению."

[sub_resource type="Resource" id="Resource_08nrb"]
script = ExtResource("2_t55ri")
speaker_name = "Нейтрофил 480"
speaker_role = "Нейтрофил"
speakers = []
line = "Похоже это грипп, прибывший через вакцину, вот почему он возник так неожиданно. Это значит, что он в разы слабее своей обычной версии."

[sub_resource type="Resource" id="Resource_l8ba1"]
script = ExtResource("2_t55ri")
speaker_name = "Нейтрофил 480"
speaker_role = "Нейтрофил"
speakers = []
line = "Вот, видишь впереди стоит одна заражённая клетка."

[sub_resource type="Resource" id="Resource_4ybn2"]
script = ExtResource("3_ltbg8")
index = null
image = ExtResource("4_g7cdx")
expression = ExtResource("3_vjrky")

[sub_resource type="Resource" id="Resource_v5yav"]
script = ExtResource("2_t55ri")
speaker_name = "Нейтрофил 763"
speaker_role = "Нейтрофил"
speakers = [SubResource("Resource_4ybn2")]
line = "Так точно."

[sub_resource type="Resource" id="Resource_tchpk"]
script = ExtResource("2_t55ri")
speaker_name = "Нейтрофил 480"
speaker_role = "Нейтрофил"
speakers = []
line = "Иди и разберись с ней, она тебе по плечу. Заодно получишь реальный опыт в нашей работе. Делай всё так, как тебя учили в костном мозге."

[sub_resource type="Resource" id="Resource_1loyl"]
script = ExtResource("3_ltbg8")
index = null
image = ExtResource("4_g7cdx")
expression = ExtResource("6_cowxw")

[sub_resource type="Resource" id="Resource_j2mu0"]
script = ExtResource("2_t55ri")
speaker_name = "Нейтрофил 763"
speaker_role = "Нейтрофил"
speakers = [SubResource("Resource_1loyl")]
line = "А вы что, не пойдёте?"

[sub_resource type="Resource" id="Resource_pp0p4"]
script = ExtResource("2_t55ri")
speaker_name = "Нейтрофил 480"
speaker_role = "Нейтрофил"
speakers = []
line = "Оглянись по сторонам. Вирусов сотни и они заражают другие клетки, хотя и не так быстро, потому что они из вакцины."

[sub_resource type="Resource" id="Resource_0y6gp"]
script = ExtResource("3_ltbg8")
