[sub_resource type="Resource" id="Resource_ny45u"]
script = ExtResource("2_t55ri")
speaker_name = "Нейтрофил 763"
speaker_role = "Нейтрофил"
speakers = [SubResource("Resource_huxpx")]
line = "Как только я вышел из костного мозга, мой щит, который мне выдали после обучения, начал светиться синим и вести меня по сосудам."

[sub_resource type="Resource" id="Resource_bpaxc"]
script = ExtResource("3_ltbg8")
index = null
image = ExtResource("4_g7cdx")
expression = ExtResource("3_vjrky")

[sub_resource type="Resource" id="Resource_a13tb"]
script = ExtResource("2_t55ri")
speaker_name = "Нейтрофил 763"
speaker_role = "Нейтрофил"
speakers = [SubResource("Resource_bpaxc")]
line = "Так, я пришёл сюда, к сооружению, которое выглядит странно - оно то увеличивается, то уменьшается."

[sub_resource type="Resource" id="Resource_ulmwb"]
script = ExtResource("3_ltbg8")
index = null
image = ExtResource("4_g7cdx")
expression = ExtResource("3_vjrky")

[sub_resource type="Resource" id="Resource_sgmpp"]
script = ExtResource("2_t55ri")
speaker_name = "Нейтрофил 763"
speaker_role = "Нейтрофил"
speakers = [SubResource("Resource_ulmwb")]
line = "Хм. Указатель щита показывает идти вперёд. Прямо внутрь этого места."

[sub_resource type="Resource" id="Resource_486nh"]
script = ExtResource("3_ltbg8")
index = null
image = ExtResource("4_g7cdx")
expression = ExtResource("3_vjrky")

[sub_resource type="Resource" id="Resource_0h5p1"]
script = ExtResource("2_t55ri")
speaker_name = "Нейтрофил 763"
speaker_role = "Нейтрофил"
speakers = [SubResource("Resource_486nh")]
line = "(Заходит внутрь)"

[sub_resource type="Resource" id="Resource_6g8tn"]
script = ExtResource("3_ltbg8")
